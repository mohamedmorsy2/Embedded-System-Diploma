#include "timer1.h"
#include "gpio.h"
#include "common_macros.h"
#include <avr/io.h>




void app(void)
{
	TOGGLE_BIT(PORTB , PIN4_ID);
}




int main()
{
	/*timer setting*/
	Timer1_ConfigType config_timer = {0 , 7813 , clkI_1024 ,CTC};

GPIO_setupPinDirection(PORTB_ID , PIN4_ID , PIN_OUTPUT);

SREG |= (1<<7);
Timer1_setCallBack(app);
Timer1_init(&config_timer);


while(1)
{

}


return 0;
}
